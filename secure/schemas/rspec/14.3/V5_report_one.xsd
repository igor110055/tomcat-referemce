<xs:schema xmlns="http://developer.cognos.com/schemas/report/14.3/" xmlns:rspec="http://developer.cognos.com/schemas/report/14.3/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://developer.cognos.com/schemas/report/14.3/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="ExpressionType">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TextType">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="PositiveUnboundedIntegerType">
		<xs:attribute name="value" type="xs:positiveInteger" use="optional"/>
	</xs:complexType>
	<xs:element name="modelPath">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="type" default="FM">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="FM"/>
								<xs:enumeration value="module"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="sortList">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="sortItem"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sortItem">
		<xs:complexType>
			<xs:attribute name="refDataItem" type="xs:string" use="required"/>
			<xs:attribute name="sortOrder" default="ascending">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ascending"/>
						<xs:enumeration value="descending"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="XMLAttributes">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="XMLAttribute">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="TextType">
								<xs:attribute name="name" type="xs:string" use="required">
								</xs:attribute>
								<xs:attribute name="value" type="xs:string" use="optional">
								</xs:attribute>
								<xs:attribute name="output" default="XML">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="no"/>
											<xs:enumeration value="XML">
											</xs:enumeration>
											<xs:enumeration value="HTML">
											</xs:enumeration>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="LocalizedTextType">
		<xs:all>
			<xs:element name="defaultText" type="TextType">
			</xs:element>
			<xs:element name="localizedText" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="localeText" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="TextType">
										<xs:attribute name="locale" type="xs:language" use="required">
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:element name="reportPath">
		<xs:complexType>
			<xs:all>
				<xs:element ref="XMLAttributes" minOccurs="0"/>
			</xs:all>
			<xs:attribute name="path" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="metadataPath">
		<xs:complexType>
			<xs:all>
				<xs:element ref="XMLAttributes" minOccurs="0"/>
			</xs:all>
			<xs:attribute name="path" type="xs:string" use="required"/>
			<xs:attribute name="type" default="FM">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="FM"/>
						<xs:enumeration value="module"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="format">
		<xs:complexType>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="stringFormat">
		<xs:complexType>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attribute name="digitShaping">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="national">
						</xs:enumeration>
						<xs:enumeration value="contextual">
						</xs:enumeration>
						<xs:enumeration value="none">
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="dateFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonDateFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="timeFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonTimeFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="dateTimeFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonDateFormatAttributes"/>
			<xs:attributeGroup ref="CommonTimeFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="intervalFormat">
		<xs:complexType>
			<xs:attribute name="units">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="time">
						</xs:enumeration>
						<xs:enumeration value="days"/>
						<xs:enumeration value="hours"/>
						<xs:enumeration value="minutes"/>
						<xs:enumeration value="seconds"/>
						<xs:enumeration value="milliseconds"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="showDays">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="false">
						</xs:enumeration>
						<xs:enumeration value="true">
						</xs:enumeration>
						<xs:enumeration value="1-digit">
						</xs:enumeration>
						<xs:enumeration value="2-digits">
						</xs:enumeration>
						<xs:enumeration value="Julian">
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="showYears" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="showMonths" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="dateSeparator" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonIntervalFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="numberFormat">
		<xs:complexType>
			<xs:attribute name="useScientific" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="exponentSize" type="xs:nonNegativeInteger">
			</xs:attribute>
			<xs:attribute name="exponentSymbol" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="currencyFormat">
		<xs:complexType>
			<xs:attribute name="currencyCode" type="xs:string">
			</xs:attribute>
			<xs:attribute name="currencySymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="intlCurrencySymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="useIntlSymbol" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="useTrailingSymbol" type="xs:boolean">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="percentFormat">
		<xs:complexType>
			<xs:attribute name="percentSymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="percentScale" type="xs:nonPositiveInteger">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
			<xs:attributeGroup ref="CommonNonStringFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="CommonFormatAttributes">
		<xs:attribute name="pattern" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenMissing" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenSecurityError" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenNotApplicable" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenAnyError" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonDateFormatAttributes">
		<xs:attribute name="showYears">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="hideCentury">
					</xs:enumeration>
					<xs:enumeration value="showCentury">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMonths">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="shortName">
					</xs:enumeration>
					<xs:enumeration value="fullName">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dateSeparator" type="xs:string">
		</xs:attribute>
		<xs:attribute name="dateStyle">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="short"/>
					<xs:enumeration value="medium"/>
					<xs:enumeration value="long"/>
					<xs:enumeration value="full"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="displayOrder">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="YMD"/>
					<xs:enumeration value="MDY"/>
					<xs:enumeration value="DMY"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showDays">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="Julian">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showWeekday">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="shortName">
					</xs:enumeration>
					<xs:enumeration value="fullName">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showEra" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="calendar">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Gregorian">
					</xs:enumeration>
					<xs:enumeration value="Imperial">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonIntervalFormatAttributes">
		<xs:attribute name="showHours">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMinutes">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showSeconds">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMilliseconds">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="3-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="timeSeparator" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonTimeFormatAttributes">
		<xs:attribute name="timeStyle">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="short"/>
					<xs:enumeration value="long"/>
					<xs:enumeration value="full"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showAmPm" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="showTimeZone" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="clock">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="12-hour">
					</xs:enumeration>
					<xs:enumeration value="24-hour">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="decimalDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attributeGroup ref="CommonIntervalFormatAttributes"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="IntegerFormatAttributes">
		<xs:attribute name="sign" type="xs:string">
		</xs:attribute>
		<xs:attribute name="useTrailingSign" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="maxDigits" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="minDigits" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="paddingCharacter" type="xs:string">
		</xs:attribute>
		<xs:attribute name="scale" type="xs:short">
		</xs:attribute>
		<xs:attribute name="useGrouping" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="groupDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attribute name="groupSize" type="xs:positiveInteger">
		</xs:attribute>
		<xs:attribute name="groupSecondarySize" type="xs:positiveInteger">
		</xs:attribute>
		<xs:attribute name="whenNegative" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenZero" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="DecimalFormatAttributes">
		<xs:attribute name="decimalSize" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="decimalDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attributeGroup ref="IntegerFormatAttributes"/>
		<xs:attribute name="whenDivideZero" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenNumericOverflow" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonNonStringFormatAttributes">
		<xs:attribute name="digitShaping">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="national">
					</xs:enumeration>
					<xs:enumeration value="none">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="style">
		<xs:complexType>
			<xs:all>
				<xs:element ref="CSS" minOccurs="0"/>
				<xs:element ref="dataFormat" minOccurs="0"/>
				<xs:element ref="defaultStyles" minOccurs="0"/>
				<xs:element ref="generatedBackground" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="conditionalStyles">
		<xs:complexType>
			<xs:all>
				<xs:element name="conditionalStyleCases">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="conditionalStyle" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element ref="CSS" minOccurs="0"/>
										<xs:element ref="dataFormat" minOccurs="0"/>
										<xs:element ref="generatedBackground" minOccurs="0"/>
									</xs:all>
									<xs:attributeGroup ref="ConditionalPartAttributes"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attributeGroup ref="ConditionalOwnerAttributes"/>
					</xs:complexType>
					<xs:unique name="styleCases">
						<xs:selector xpath="*"/>
						<xs:field xpath="@refVariableValue"/>
					</xs:unique>
				</xs:element>
				<xs:element name="conditionalStyleDefault">
					<xs:complexType>
						<xs:all>
							<xs:element ref="CSS" minOccurs="0"/>
							<xs:element ref="dataFormat" minOccurs="0"/>
							<xs:element ref="generatedBackground" minOccurs="0"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CSS">
		<xs:complexType>
			<xs:attribute name="value" type="xs:string" use="required">
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="dataFormat">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="numberFormat"/>
				<xs:element ref="percentFormat"/>
				<xs:element ref="currencyFormat"/>
				<xs:element ref="stringFormat"/>
				<xs:element ref="dateFormat"/>
				<xs:element ref="dateTimeFormat"/>
				<xs:element ref="timeFormat"/>
				<xs:element ref="intervalFormat"/>
				<xs:element ref="format"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="defaultStyles">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="defaultStyle" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="refStyle" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="generatedBackground">
		<xs:complexType>
			<xs:all>
				<xs:element name="border" minOccurs="0">
					<xs:complexType>
						<xs:attribute name="borderColor" type="ColorType" default="gray"/>
						<xs:attribute name="borderStyle" default="solid">
							<xs:simpleType>
								<xs:restriction base="xs:NMTOKEN">
									<xs:enumeration value="solid"/>
									<xs:enumeration value="double"/>
									<xs:enumeration value="dotted"/>
									<xs:enumeration value="dashed"/>
									<xs:enumeration value="groove"/>
									<xs:enumeration value="ridge"/>
									<xs:enumeration value="inset"/>
									<xs:enumeration value="outset"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="borderWidth" type="CSSLength" default="1pt">
						</xs:attribute>
						<xs:attribute name="cornerRadius" type="CSSLength" default="0pt">
						</xs:attribute>
						<xs:attribute name="allowTransparentBleed" type="xs:boolean" default="false">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="dropShadow" minOccurs="0">
					<xs:complexType>
						<xs:attribute name="shadowColor" type="ColorType" default="gray"/>
						<xs:attribute name="thickness" type="CSSLength" default="5px"/>
						<xs:attributeGroup ref="TransparencyAttributes"/>
						<xs:attribute name="shadowBlur" default="none">
							<xs:simpleType>
								<xs:restriction base="xs:NMTOKEN">
									<xs:enumeration value="none">
									</xs:enumeration>
									<xs:enumeration value="minimumBlur"/>
									<xs:enumeration value="littleBlur"/>
									<xs:enumeration value="averageBlur"/>
									<xs:enumeration value="muchBlur"/>
									<xs:enumeration value="maximumBlur"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="backgroundImages" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="backgroundImage" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="backgroundImagePosition">
											<xs:complexType>
