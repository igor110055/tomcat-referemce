<xs:schema xmlns="http://developer.cognos.com/schemas/report/3.0/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://developer.cognos.com/schemas/report/3.0/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:simpleType name="ExpressionType">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TextType">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="PositiveUnboundedIntegerType">
		<xs:attribute name="value" type="xs:positiveInteger" use="optional"/>
	</xs:complexType>
	<xs:element name="modelPath" type="xs:string">
	</xs:element>
	<xs:element name="sortList">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="sortItem"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="sortItem">
		<xs:complexType>
			<xs:attribute name="refDataItem" type="xs:string" use="required"/>
			<xs:attribute name="sortOrder" default="ascending">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="ascending"/>
						<xs:enumeration value="descending"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="format">
		<xs:complexType>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="stringFormat">
		<xs:complexType>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="dateFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonDateFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="timeFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonTimeFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="dateTimeFormat">
		<xs:complexType>
			<xs:attribute name="whenZero" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonDateFormatAttributes"/>
			<xs:attributeGroup ref="CommonTimeFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="intervalFormat">
		<xs:complexType>
			<xs:attribute name="units">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="time"/>
						<xs:enumeration value="days"/>
						<xs:enumeration value="hours"/>
						<xs:enumeration value="minutes"/>
						<xs:enumeration value="seconds"/>
						<xs:enumeration value="milliseconds"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="showDays">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="false">
						</xs:enumeration>
						<xs:enumeration value="true">
						</xs:enumeration>
						<xs:enumeration value="1-digit">
						</xs:enumeration>
						<xs:enumeration value="2-digits">
						</xs:enumeration>
						<xs:enumeration value="Julian">
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="showYears" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="showMonths" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="dateSeparator" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="CommonIntervalFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="numberFormat">
		<xs:complexType>
			<xs:attribute name="useScientific" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="exponentSize" type="xs:nonNegativeInteger">
			</xs:attribute>
			<xs:attribute name="exponentSymbol" type="xs:string">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="currencyFormat">
		<xs:complexType>
			<xs:attribute name="currencyCode" type="xs:string">
			</xs:attribute>
			<xs:attribute name="currencySymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="intlCurrencySymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="useIntlSymbol" type="xs:boolean">
			</xs:attribute>
			<xs:attribute name="useTrailingSymbol" type="xs:boolean">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="percentFormat">
		<xs:complexType>
			<xs:attribute name="percentSymbol" type="xs:string">
			</xs:attribute>
			<xs:attribute name="percentScale" type="xs:nonPositiveInteger">
			</xs:attribute>
			<xs:attributeGroup ref="CommonFormatAttributes"/>
			<xs:attributeGroup ref="DecimalFormatAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="CommonFormatAttributes">
		<xs:attribute name="pattern" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenMissing" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenSecurityError" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenNotApplicable" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenAnyError" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonDateFormatAttributes">
		<xs:attribute name="showYears">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="hideCentury">
					</xs:enumeration>
					<xs:enumeration value="showCentury">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMonths">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="shortName">
					</xs:enumeration>
					<xs:enumeration value="fullName">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dateSeparator" type="xs:string">
		</xs:attribute>
		<xs:attribute name="dateStyle">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="short"/>
					<xs:enumeration value="medium"/>
					<xs:enumeration value="long"/>
					<xs:enumeration value="full"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="displayOrder">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="YMD"/>
					<xs:enumeration value="MDY"/>
					<xs:enumeration value="DMY"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showDays">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="Julian">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showWeekday">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="shortName">
					</xs:enumeration>
					<xs:enumeration value="fullName">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showEra" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="calendar">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Gregorian">
					</xs:enumeration>
					<xs:enumeration value="Imperial">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonIntervalFormatAttributes">
		<xs:attribute name="showHours">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMinutes">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showSeconds">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showMilliseconds">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="false">
					</xs:enumeration>
					<xs:enumeration value="true">
					</xs:enumeration>
					<xs:enumeration value="1-digit">
					</xs:enumeration>
					<xs:enumeration value="2-digits">
					</xs:enumeration>
					<xs:enumeration value="3-digits">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="timeSeparator" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CommonTimeFormatAttributes">
		<xs:attribute name="timeStyle">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="short"/>
					<xs:enumeration value="long"/>
					<xs:enumeration value="full"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="showAmPm" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="showTimeZone" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="clock">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="12-hour">
					</xs:enumeration>
					<xs:enumeration value="24-hour">
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="decimalDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attributeGroup ref="CommonIntervalFormatAttributes"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="IntegerFormatAttributes">
		<xs:attribute name="sign" type="xs:string">
		</xs:attribute>
		<xs:attribute name="useTrailingSign" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="maxDigits" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="minDigits" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="paddingCharacter" type="xs:string">
		</xs:attribute>
		<xs:attribute name="scale" type="xs:short">
		</xs:attribute>
		<xs:attribute name="useGrouping" type="xs:boolean">
		</xs:attribute>
		<xs:attribute name="groupDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attribute name="groupSize" type="xs:positiveInteger">
		</xs:attribute>
		<xs:attribute name="groupSecondarySize" type="xs:positiveInteger">
		</xs:attribute>
		<xs:attribute name="whenNegative" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenZero" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="DecimalFormatAttributes">
		<xs:attribute name="decimalSize" type="xs:nonNegativeInteger">
		</xs:attribute>
		<xs:attribute name="decimalDelimiter" type="xs:string">
		</xs:attribute>
		<xs:attributeGroup ref="IntegerFormatAttributes"/>
		<xs:attribute name="whenDivideZero" type="xs:string">
		</xs:attribute>
		<xs:attribute name="whenNumericOverflow" type="xs:string">
		</xs:attribute>
	</xs:attributeGroup>
	<xs:element name="style" type="StyleType">
	</xs:element>
	<xs:element name="conditionalStyles">
		<xs:complexType>
			<xs:all>
				<xs:element name="conditionalStyleCases">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="conditionalStyle" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="StyleType">
											<xs:attributeGroup ref="ConditionalPartAttributes"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attributeGroup ref="ConditionalOwnerAttributes"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="conditionalStyleDefault" type="StyleType"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CSS">
		<xs:complexType>
			<xs:attribute name="value" type="xs:string" use="required">
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="dataFormat">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="numberFormat"/>
				<xs:element ref="percentFormat"/>
				<xs:element ref="currencyFormat"/>
				<xs:element ref="stringFormat"/>
				<xs:element ref="dateFormat"/>
				<xs:element ref="dateTimeFormat"/>
				<xs:element ref="timeFormat"/>
				<xs:element ref="intervalFormat"/>
				<xs:element ref="format"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:attributeGroup name="ConditionalPartAttributes">
		<xs:attribute name="refVariableValue" type="xs:string" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ConditionalOwnerAttributes">
		<xs:attribute name="refVariable" type="xs:string" use="required"/>
	</xs:attributeGroup>
	<xs:complexType name="StyleType">
		<xs:all>
			<xs:element ref="CSS" minOccurs="0"/>
			<xs:element ref="dataFormat" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:element name="conditionalRender">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="renderFor" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attributeGroup ref="ConditionalPartAttributes"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="ConditionalOwnerAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="XMLAttributes">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="XMLAttribute">
					<xs:complexType>
						<xs:attribute name="name" type="xs:string" use="required">
						</xs:attribute>
						<xs:attribute name="value" type="xs:string" use="required">
						</xs:attribute>
						<xs:attribute name="render" type="xs:boolean" default="true">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="dataSource" type="DataSourceType">
	</xs:element>
	<xs:element name="conditionalDataSources">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="conditionalDataSource" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="DataSourceType">
								<xs:attributeGroup ref="ConditionalPartAttributes"/>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="ConditionalOwnerAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="reportExpression" type="ExpressionType">
	</xs:element>
	<xs:element name="defaultMeasure">
		<xs:complexType>
			<xs:attribute name="refDataItem" type="xs:string" use="required"/>
			<xs:attributeGroup ref="SolveOrderAttributes"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="reportDrills">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="reportDrill" maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<xs:element name="drillTarget" minOccurs="0">
								<xs:complexType>
									<xs:all>
										<xs:element ref="reportPath"/>
										<xs:element name="drillLinks" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="drillLink" maxOccurs="unbounded">
														<xs:complexType>
															<xs:all>
																<xs:element name="drillSourceContext" minOccurs="0">
																	<xs:complexType>
																		<xs:choice>
																			<xs:element ref="dataItemContext"/>
																			<xs:element ref="parameterContext"/>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
																<xs:element name="drillTargetContext">
																	<xs:complexType>
																		<xs:choice>
																			<xs:element ref="parameterContext"/>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
															</xs:all>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:all>
									<xs:attribute name="showInNewWindow" type="xs:boolean" default="false">
									</xs:attribute>
									<xs:attribute name="method" use="optional">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="edit"/>
												<xs:enumeration value="execute"/>
												<xs:enumeration value="view"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="outputFormat" use="optional">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="CSV"/>
												<xs:enumeration value="HTML"/>
												<xs:enumeration value="HTMLFragment"/>
												<xs:enumeration value="PDF"/>
												<xs:enumeration value="XHTML"/>
												<xs:enumeration value="XLS"/>
												<xs:enumeration value="XLWA"/>
												<xs:enumeration value="singleXLS"/>
												<xs:enumeration value="XML"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="bookmarkRef" minOccurs="0">
								<xs:complexType>
									<xs:all>
										<xs:element ref="dataSource"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="drillLabel">
								<xs:complexType>
									<xs:all>
										<xs:element ref="dataSource"/>
										<xs:element ref="conditionalDataSources" minOccurs="0"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:all>
						<xs:attribute name="name" type="xs:string" use="required">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="masterDetailLinks">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="masterDetailLink" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<xs:element name="masterContext">
								<xs:complexType>
									<xs:choice>
										<xs:element ref="dataItemContext"/>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="detailContext">
								<xs:complexType>
									<xs:choice>
										<xs:element ref="dataItemContext"/>
										<xs:element ref="parameterContext"/>
									</xs:choice>
								</xs:complexType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="reportPath">
		<xs:complexType>
			<xs:all>
				<xs:element ref="XMLAttributes" minOccurs="0"/>
			</xs:all>
			<xs:attribute name="path" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="dataItemContext">
		<xs:complexType>
			<xs:attribute name="refDataItem" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="parameterContext">
		<xs:complexType>
			<xs:attribute name="parameter" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="propertyList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="propertyItem" maxOccurs="unbounded">
					<xs:complexType>
						<xs:attribute name="refDataItem" type="xs:string" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="DataSourceType">
		<xs:choice>
			<xs:element name="staticValue">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="preserve"/>
					</xs:restriction>
